{
  "expo": {
    "name": "cgcrush",
    "slug": "cgcrush",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme":"https",
    "icon": "./assets/logo.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cgcrush.cgcrush",
      "config": {
        "googleSignIn":{
          "apiKey":"Api key here",
          "certificateHash": "has here"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
